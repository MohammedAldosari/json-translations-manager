{
	"name": "json-translations-manager",
	"displayName": "JSON Translations Editor",
	"description": "Edit your translation files in parallel. This extension help the developers to manage the app translations",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json-translations-manager.translate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-translations-manager.translate",
				"title": "Adding translation to your i18n json files"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/papaparse": "^5.0.4",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"jsonfile": "^6.0.1",
		"papaparse": "^5.2.0"
	}
}
